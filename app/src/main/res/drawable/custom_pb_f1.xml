<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--Top Left-->
    <item
        android:width="16dp"
        android:height="16dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF000000" />
        </shape>
    </item>
    <!--Top Right-->
    <item
        android:width="16dp"
        android:height="16dp"
        android:left="16dp">
        <shape android:shape="rectangle">
            <solid android:color="#33000000" />
        </shape>
    </item>
    <!--Bottom Left-->
    <item
        android:width="16dp"
        android:height="16dp"
        android:top="16dp">
        <shape android:shape="rectangle">
            <solid android:color="#BB000000" />
        </shape>
    </item>
    <!--Bottom Right-->
    <item
        android:width="16dp"
        android:height="16dp"
        android:left="16dp"
        android:top="16dp">
        <shape android:shape="rectangle">
            <solid android:color="#77000000" />
        </shape>
    </item>
</layer-list>