<?xml version="1.0" encoding="utf-8"?><?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!--Top Left Vertical-->
    <item
        android:width="8dp"
        android:height="24dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/black" />
        </shape>
    </item>

    <!--Top Left Horizontal-->
    <item
        android:width="24dp"
        android:height="8dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/black" />
        </shape>
    </item>

    <!--Top Right Vertical-->
    <item
        android:width="8dp"
        android:height="24dp"
        android:gravity="top|right">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/black" />
        </shape>
    </item>

    <!--Top Right Horizontal-->
    <item
        android:width="24dp"
        android:height="8dp"
        android:gravity="top|right">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/black" />
        </shape>
    </item>

    <!--Bottom Left Vertical-->
    <item
        android:width="8dp"
        android:height="24dp"
        android:gravity="bottom|left">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/black" />
        </shape>
    </item>

    <!--Bottom Left Horizontal-->
    <item
        android:width="24dp"
        android:height="8dp"
        android:gravity="bottom|left">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/black" />
        </shape>
    </item>

    <!--Bottom Right Vertical-->
    <item
        android:width="8dp"
        android:height="24dp"
        android:gravity="bottom|right">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/black" />
        </shape>
    </item>

    <!--Bottom Right Horizontal-->
    <item
        android:width="24dp"
        android:height="8dp"
        android:gravity="bottom|right">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/black" />
        </shape>
    </item>


</layer-list>